{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2023",
    "outDir": "dist",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"]
    },
    "typeRoots": ["src/types", "node_modules/@types"]
  },
  "files": ["src/types/type.d.ts"],
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
